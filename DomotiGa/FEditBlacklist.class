PUBLIC SUB Form_Open()

  ME.Move(FMain.X + 50, FMain.Y + 70)

  WITH DataBrowser1.GridView
    .Columns[0].Text = "Id"
    .Columns[0].Width = 28
    .Columns[1].Text = ("Address")
    .Columns[1].Width = 130
    .Columns[2].Text = ("Comments")
    .Columns[2].Width = 130
  END WITH

END

PUBLIC SUB btnClose_Click()

  ME.Close

END
