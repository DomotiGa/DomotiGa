PUBLIC SUB Form_Open()

  ME.Move(FMain.X + 50, FMain.Y + 70)

  WITH DataBrowser1.GridView
    .Columns[0].Text = "Id"
    .Columns[0].Width = 28
    .Columns[1].Text = ("Name")
    .Columns[1].Width = 80
    .Columns[2].Text = ("Url")
    .Columns[2].Width = 110
    .Columns[3].Text = ("Enabled")
    .Columns[3].Width = 60
    .Columns[4].Text = ("Description")
  END WITH

END

PUBLIC SUB btnClose_Click()

  ME.Close

END


