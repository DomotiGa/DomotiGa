  sCt2 = CStr(((RecBuf[9] * 256) + RecBuf[10]) / 10)
  WriteMessage("Channel 2     = " & sCt2 & " ampere")
  sCt3 = CStr(((RecBuf[11] * 256) + RecBuf[12]) / 10)
  WriteMessage("Channel 3     = " & sCt3 & " ampere")
