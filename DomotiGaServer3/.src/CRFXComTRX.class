  IF iDeviceId THEN
    ' ct1 amp, ct2 amp, ct3 amp
    Devices.ValueUpdate(iDeviceId, Format(sCt1, "##.###"), Format(sCt2, "##.###"), Format(sCt3, "##.###"), "")
    Devices.Battery(iDeviceId, sBatt)
  ENDIF

