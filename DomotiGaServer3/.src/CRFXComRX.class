    sDir = "SE"
  Else If iDirection < 168.76 Then
    sDir = "SSE"
  Else If iDirection < 191.26 Then
    sDir = "S"
  Else If iDirection < 213.76 Then
    sDir = "SSW"
  Else If iDirection < 236.26 Then
    sDir = "SW"
  Else If iDirection < 258.76 Then
