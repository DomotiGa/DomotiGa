    Endif
    sUv = CSingle(Hex(RecBuf[5] And &HF)) * 10 + CSingle(Hex(Lsr(RecBuf[4], 4)))
    If $bRFXComRXDebug Then Main.WriteRFXDebugLog(" UV factor:" & CStr(sUv) & " Level:", True)
    If sUv < 3 Then
      sUvText = ("Low")
    Else If sUv < 6 Then
      sUvText = ("Medium")
    Else If sUv < 8 Then
