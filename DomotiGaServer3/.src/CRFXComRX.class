    If ((RecBuf[5] And &HF0) < &HA0) And ((RecBuf[5] And &HF) < &HA) And ((RecBuf[4] And &HF0) < &HA0) Then
      If $bRFXComRXDebug Then Main.WriteRFXDebugLog(" temp:", 1)
