  Checksum += (Lsr(RecBuf[8], 4) And &HF) + (RecBuf[8] And &HF)
  Checksum += (Lsr(RecBuf[9], 4) And &HF) + (RecBuf[9] And &HF)
  Checksum += (Lsr(RecBuf[10], 4) And &HF) + (RecBuf[10] And &HF)
  Checksum = (Checksum - RecBuf[11]) And &HFF
  If Checksum <> 0 Then
