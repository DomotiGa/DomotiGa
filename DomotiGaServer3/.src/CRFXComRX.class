    If $bRFXComRXDebug Then Main.WriteRFXDebugLog(" direction " & CStr(iDirection) & "°", True)
    sWrDirection = WrDirection(iDirection)
    sSpeed = (CSingle(Hex(RecBuf[7] And &HF)) * 10) + (CSingle(Hex(RecBuf[6])) / 10)
