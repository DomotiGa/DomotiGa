Public Sub ChangeGrid(gv As GridView, i As Integer, pos As Integer)

  Dim k As Integer
  Dim row As New String[]

  ' load first data into row
  For k = 0 To gv.Columns.Count - 1
    row.Add(gv[i, k].Text)
  Next

  ' put second row into first
  For k = 0 To gv.Columns.Count - 1
    gv[i, k].Text = gv[pos, k].Text
