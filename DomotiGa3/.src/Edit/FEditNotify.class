Public Sub Form_Open()

  Me.Move(FMain.X + 50, FMain.Y + 70)

  With DataBrowser1.View
    .Columns[0].Text = ("Id")
    .Columns[0].Width = 28
    .Columns[1].Text = ("Keyword")
    .Columns[1].Width = 150
  End With

End

Public Sub btnClose_Click()

  Me.Close

End
