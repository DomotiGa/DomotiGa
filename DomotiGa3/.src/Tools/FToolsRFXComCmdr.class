'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
' Blinds1 commands
'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
Private Sub Command_Blinds1(sCommand As String)

  Dim cData As New JSONCollection
  Dim aType As New String[]

  aType = Split(cmbBlinds1Type.Text, " ")
  If aType.Count < 2 Then
    Return
  Endif

  If Main.bServer Then
    $hRFXComTRX.Blinds1(aType[0], tbBlinds1Address.Text, sCommand)
  Else
    cData["rfxcom"] = "rfxcomtrx"
    cData["instance_id"] = $TRXInstanceList[cmbTRXList.Index]
    cData["function"] = "blinds1"
    cData["type"] = aType[0]
    cData["address"] = tbBlinds1Address.Text
    cData["command"] = sCommand
    Client.Call_Tools_RFXCom_Set(cData)
  Endif

End

