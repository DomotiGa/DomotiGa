Public Sub Form_Open()

  Me.Move(FMain.X + 50, FMain.Y + 70)

  ' [GB2:DBGV] WITH DataBrowser1.GridView
  With DataBrowser1.View
    .Columns[0].Text = ("Id")
    .Columns[0].Width = 28
    .Columns[1].Text = ("Name")
    .Columns[1].Width = 150
  End With

End

Public Sub btnClose_Click()

  Me.Close

End
