      WriteDebugLog("Sequence no.  = " & CStr(RecBuf[3]))
      WriteDebugLog("Housecode     = " & Chr(RecBuf[4]))
      WriteDebugLog("Unitcode      = " & CStr(RecBuf[5]))
      sAddress = Chr(RecBuf[4]) & Format(CStr(RecBuf[5]), "00")
      Select Case RecBuf[6]
