  WriteDebugLog("Sequence no.  = " & CStr(RecBuf[3]))
  sAddress = ConvertAddress(sType & " " & (RecBuf[4] * 256 + RecBuf[5]))
  WriteDebugLog("ID            = " & sAddress)
